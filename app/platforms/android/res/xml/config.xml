<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tcz.httpReq" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SocketsForCordova">
        <param name="android-package" value="cz.blocshop.socketsforcordova.SocketPlugin" />
    </feature>
    <allow-intent href="market:*" />
    <name>httpReq</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access launch-external="yes" origin="*" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
</widget>
