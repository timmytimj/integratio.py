/*
* Example of Delay test configuration
*
*/

{
    "test-id"   : "General test 1",
    "interface" : "eth0",
    "tcp-port"  : 80,
    "configs"     : [
        {
            /* TIME */
            "category"  : "time",
            "parameters": [
                {
                    "state"     : "ESTABLISHED",
                    "action"    : "sendAck",
                    "delay"     : 6
                },
                {
                    "state"     : "CLOSING",
                    "action"    : "CLOSING",
                    "delay"     : 2
                }
            ]
        }
    ]
}
